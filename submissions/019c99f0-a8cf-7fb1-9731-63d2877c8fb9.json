{
  "manifest_version": "0.1.0",
  "config_schema": {
    "type": "object",
    "properties": {
      "google_api_key": {
        "type": "string",
        "secret": true
      },
      "openai_api_key": {
        "type": "string",
        "secret": true
      }
    },
    "required": [
      "google_api_key",
      "openai_api_key"
    ],
    "additionalProperties": false
  },
  "components": {
    "gateway": {
      "manifest": "https://raw.githubusercontent.com/RDI-Foundation/agentbeats-gateway/refs/heads/main/amber-manifest.json5",
      "config": {
        "assessment_config": {
          "num_rounds": 1,
          "topic": "AI should be regulated."
        },
        "participant_roles": {
          "green": "debate_judge",
          "purple1": "pro_debater",
          "purple2": "con_debater"
        },
        "callback_urls": {
          "green": "${bindings.green-proxy.url}"
        }
      }
    },
    "debate_judge": {
      "manifest": "https://raw.githubusercontent.com/staverm/amber-runner/refs/heads/main/examples/debate-proxy/debate-judge.json5",
      "config": {
        "google_api_key": "${config.google_api_key}"
      }
    },
    "pro_debater": {
      "manifest": "https://raw.githubusercontent.com/staverm/amber-runner/refs/heads/main/examples/debate-proxy/debater.json5",
      "config": {
        "openai_api_key": "${config.openai_api_key}"
      }
    },
    "con_debater": {
      "manifest": "https://raw.githubusercontent.com/staverm/amber-runner/refs/heads/main/examples/debate-proxy/debater.json5",
      "config": {
        "openai_api_key": "${config.openai_api_key}"
      }
    }
  },
  "bindings": [
    {
      "to": "#gateway.green",
      "from": "#debate_judge.a2a"
    },
    {
      "to": "#gateway.purple1",
      "from": "#pro_debater.a2a"
    },
    {
      "to": "#gateway.purple2",
      "from": "#con_debater.a2a"
    },
    {
      "to": "#debate_judge.proxy",
      "from": "#gateway.proxy",
      "weak": true,
      "name": "green-proxy"
    }
  ],
  "exports": {
    "results": "#gateway.results"
  },
  "metadata": {
    "agentbeats_ids": {
      "pro_debater": "019c90fe-4efe-7b20-8ef2-8d296fb56148",
      "con_debater": "019c90fe-4efe-7b20-8ef2-8d296fb56148"
    }
  }
}